.about-content {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -o-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  padding: .8125rem .9375rem .8125rem;
  color: #000;
  font-family: "Times New Roman";
}

.title-0 {
  color: #131313;
  font-size: 1.125rem;
  font-weight: 400;
  line-height: 1.125rem;
}

.box-theme-default .paragraph-0 {
  padding-top: .5rem;
}

.box-theme-default .paragraph-0, .box-theme-default .vipme-address {
  font-style: normal;
  font-size: .6875rem;
  line-height: 1rem;
}

.box-theme-default .title-0 .emphasized {
  color: #ff7514;
  font-style: normal;
}

.about-title{
  color: #ff7514;
  font-size 1rem
  font-weight bolder
}